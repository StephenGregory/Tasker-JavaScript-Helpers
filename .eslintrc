{

    "extends": "eslint:recommended",
    "env": {
        "browser": true
    },
    "rules": {
        "camelcase": [2, {"properties": "always"}],
        "curly": 2,
        "eqeqeq": 2,
        "eol-last": 1,
        "no-alert": 2,
        "no-console": 2,
        "no-debugger": 2,
        "no-extra-semi": 1,
        "no-multiple-empty-lines": [2, {"max": 1}],
        "no-undef": 2,
        "no-unneeded-ternary": 2,
        "quotes": [2, "single"],
        "yoda": [2, "never"]
    }
}
